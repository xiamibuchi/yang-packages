{
  "name": "yang-packages",
  "private": true,
  "version": "1.0.0",
  "description": "base utils and components",
  "main": "index.js",
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "prepare": "husky install",
    "preinstall": "node ./scripts/preinstall.js",
    "postinstall": "pnpm stub",
    "stub": "turbo stub --parallel",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "clear": "find . \\( -name 'node_modules' -o -name 'dist' -o -name '.nuxt' -o -name '.output' \\) -type d -exec rm -rf {} +",
    "docs:build": "pnpm --filter @syseven/docs build",
    "test": "pnpm run -r unit:test && vitest --config ./vitest.config.ts",
    "test:coverage": "vitest run --coverage --config ./vitest.config.ts"
  },
  "lint-staged": {
    "*.{ts,js,vue,tsx,jsx,md}": [
      "eslint --fix"
    ]
  },
  "keywords": [],
  "author": "ShenYang",
  "license": "MIT",
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "volta": {
    "node": "20.11.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@esbuild-kit/cjs-loader": "^2.4.4",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@syseven/constants": "workspace:*",
    "@syseven/hooks": "workspace:*",
    "@syseven/locale": "workspace:*",
    "@syseven/style": "workspace:*",
    "@syseven/utils": "workspace:*",
    "@syseven/video-player": "workspace:*",
    "@syseven/vue-components": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^21.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.69",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.4.0",
