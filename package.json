    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsonc": "^2.14.1",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.23.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
